{
  "subscriptions": [
    {
      "id": "subscription_id",
      "awsRegion": "ca-central-1",
      "logGroupName": "LOG_GROUP_NAME",

      "startTimeIso": "2021-01-01T00:00:00Z",
      "startTimeSecondsAgo": 3600,
      "endTimeIso": "2021-01-10T00:00:00Z",
      "endTimeSecondsAgo": 0,
      
      "filterPattern": null,
      "logStreamNamePrefix": "LOG_STREAM_NAME_PREFIX",
      "logStreamNames": ["LOG_STREAM_1", "LOG_STREAM_2"],
      "readMaxBatchSize": 10000,
      "minIntervalSeconds": 15,
      "maxIntervalSeconds": 60,
      "clockSkewProtectionSeconds": 15,
      
      "targetUrl": "http://localhost:7171/",
      "targetTimeoutSeconds": 120,
      "targetMaxBatchSize": 10000
    }
  ],
  "progressDbPath": "",
  "logFolderPath": "",
  "enableDebugConsoleLog": true,
  "enableDebugFileLog": true
}
